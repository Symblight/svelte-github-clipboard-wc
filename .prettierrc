{
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "quoteProps": "consistent",
  "trailingComma": "es5",
  "endOfLine": "lf",



  "plugins": [
    "./node_modules/prettier-plugin-svelte"
  ],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte",
        "svelteSortOrder": "options-styles-scripts-markup",
        "svelteBracketNewLine": true
      }
    }
  ]
}
